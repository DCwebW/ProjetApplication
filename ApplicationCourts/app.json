
{
  "expo": {
    "name": "essai",
    "slug": "applicationcourts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/png/logo-no-background.png",
          "color": "black"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dcwebw.essai",
      "config": {
        "googleMapsApiKey": "AIzaSyAXbU0K6AqYgyom0g9Y1ZoT8TvV_0CZYmQ"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dcwebw.essai",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAXbU0K6AqYgyom0g9Y1ZoT8TvV_0CZYmQ"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d9038241-dbd4-4344-af14-d641b9471a19"
      }
    },
    "owner": "dcwebw"
  }
}
